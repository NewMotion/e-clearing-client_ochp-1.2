						<s:annotation>
							<s:documentation>
								The value the EVSP is charged for. The unit of the value depends on the billingItem
							</s:documentation>
						</s:annotation>
					</s:element>
					<s:element name="currency" minOccurs="1" maxOccurs="1">
						<s:annotation>
							<s:documentation>
								Alphabetic. The displayed and charged currency. Defined in ISO 4217 - Table A.1, alphabetic list.
							</s:documentation>
						</s:annotation>
						<s:simpleType>
							<s:restriction base="s:string">
								<s:whiteSpace value="collapse"></s:whiteSpace>
							</s:restriction>
						</s:simpleType>
					</s:element>
					<s:element name="itemPrice" type="s:float" minOccurs="1" maxOccurs="1">
						<s:annotation>
							<s:documentation>
