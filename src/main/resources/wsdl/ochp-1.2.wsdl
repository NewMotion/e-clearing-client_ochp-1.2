			<s:complexType name="CdrPeriodType">
				<s:sequence>
					<s:element name="startDateTime" type="tns:LocalDateTimeType" minOccurs="1" maxOccurs="1">
						<s:annotation>
							<s:documentation>
								Starting time of period. Must be equal or later than startDateTime of the CDRInfo
							</s:documentation>
						</s:annotation>
					</s:element>
					<s:element name="endDateTime" type="tns:LocalDateTimeType" minOccurs="1" maxOccurs="1">
						<s:annotation>
							<s:documentation>
								Ending time of the period. Must be equal or earlier than endDateTime of the CDRInfo
							</s:documentation>
						</s:annotation>
